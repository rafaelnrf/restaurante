*{margin: 0; padding: 0;}
body {
    overflow-x:hidden;
}
#status {
    display:none;
}
#wrapper {
    position:relative;
    float:left;
}
.menu{
    float: left;
    width: 80%;
    height: 200px;
    margin-left: -100%;
}
.menu-on{
    margin-left: 0px;
    -moz-transition:margin-left 1s;
    -webkit-transition:margin-left 1s;
    -o-transition:margin-left 1s;
    transition:margin-left 1s;
}
.menu-off{
    margin-left: -80%;
    -moz-transition:margin-left 1s;
    -webkit-transition:margin-left 1s;
    -o-transition:margin-left 1s;
    transition:margin-left 1s;
}
.wrapper-on {
    margin-right:-80%;
}
        
#slidemenu {
    position:absolute;
    display:none;
}
figure {
    margin:0;
    padding:0;
    position:relative;
}
figure a{
    position:relative;
}
 #jqt .toolbar {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-box-shadow: rgba(0, 0, 0, 0.4) 0 1px 6px;
    box-shadow: rgba(0, 0, 0, 0.4) 0 1px 6px;
    border-bottom: 1px solid #2a3441;
    z-index: 10;
    position: relative;
    padding: 10px;
    background: url(../img/mobile-menu-bg.jpg) repeat-x;
}
 .toggle{
    cursor: pointer;
    position: relative;
    z-index: 99999999;
    background: url(../img/mobile-menu.png) no-repeat;
}
.menu-bt, .mobile-logo, .search {
    position: relative;
    display: inline-block;
}
.menu-bt {
    
}
.search {
    background: url(../img/mobile-search.png) no-repeat;
    border: 0;
    float: right;
    color:#666;
    font-weight:bold;
    font-family: Arial, Helvetica, sans-serif;
}
.bt-search {
    background: url(../img/bt-search.png) no-repeat;
    border: 0;
    float: right;
    cursor:pointer;
}
#tv-home {
    margin: 0;
    padding: 0;
}
ul.ui-swipeslide-bullets {
    position: absolute;
    background: none !important;
    border: none !important;
    width: 100%;
}
figcaption {
    position: absolute;
    left: 0;
    padding: 10px 20px;
    color: #fff;
    background-color: rgba(0,0,0,0.7);
    z-index: 999999;
    float: left;
    margin-top: -56px;
    font-weight:bold;
}
.ui-swipeslide-bullets li {
    background: url(../img/bullets.png) no-repeat;
    border: none !important;
    border-radius: 0 !important;
    padding: 0 !important;
}
.ui-swipeslide-bullets li.active {
    background: url(../img/bullets-active.png) no-repeat;
}
#jqt h2{
    font-weight: 900;
    font-family: "Arial Black", Arial, sans-serif;
    display: inline-block;
}
#home h2 {
    background: url(../img/icon-proximos.png) no-repeat;
    color: #dd0048;
}
#jqt ul#moteis-premium li {
    border-bottom: 1px solid #e2e2e2;
    background: url(../img/premium-bg.png) repeat-x;      
}
.premium-left {
    float: left;
    width: 28%;
    background: url(../img/premium-left.png) no-repeat;
}
.premium-left img {
    float: left;
    height: auto;
    clear: both;
}
.premium-left .distancia {
    float: left;
    clear: both;
    color: #282828;
    font-family: Arial, Helvetica, sans-serif;  
}
.premium-right {
    float: left;
    width: 72%;
    color: #282828;
}
.premium-right .nome {
    float: left;
    clear: both;
    font-weight: bold;
}
.premium-right .endereco {
    float: left;
    clear: both;
}
#go-site .versao-desktop {
    background: url(../img/versao-desktop.png) no-repeat;
    border:none !important;
}
#go-site .topo {
    background: url(../img/topo-da-pagina.png) no-repeat;
    border:none !important;
}
#go-site .versao-desktop a, #go-site .topo a{
    width:100%;
}
#login {
    background:#f5f5f5;
}
#login h2{
    color:#dd0048;
}
#jqt ul li input[type="email"], #jqt ul li input[type="password"] {
    width:92% !important;
    margin-left: 4%  !important;
    border-right:2px solid #cecece;
    border-left:2px solid #cecece;
    border-top:4px solid #cecece;
    border-bottom:2px solid #e0e0e0;
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    border-radius: 8px;
    background: rgb(245,245,245); /* Old browsers */
    background: -moz-linear-gradient(top,  rgba(245,245,245,1) 0%, rgba(255,255,255,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(245,245,245,1)), color-stop(100%,rgba(255,255,255,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(245,245,245,1) 0%,rgba(255,255,255,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(245,245,245,1) 0%,rgba(255,255,255,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(245,245,245,1) 0%,rgba(255,255,255,1) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(245,245,245,1) 0%,rgba(255,255,255,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f5f5f5', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
    color:#666;
}
.forgotpass {
    background: url(../img/bt-recuperar-senha.png) no-repeat;
}
#btnFazerLogin {
    background: url(../img/bt-continuar-login.png) no-repeat;
    border:none;
    cursor:pointer;
}
.f-l {
    float:left;
}
.f-r {
    float:right;
}
.info ul.links {
    display: inline-block;
    position: relative;
    text-align: center;
}
.info ul.links li, .info ul.links li a{
   color:#282828;
   font-family:Arial;
   font-weight:normal;
   line-height: initial;
   padding:0 5px 0 5px;
}
