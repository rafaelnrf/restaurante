        background-size: 252px 44px;
        text-indent: 5px;
        left: 2px;
        top: 14px;
    }
    .bt-search {
        width: 51px;
        height: 41px;
        background-size: 51px 41px;
        margin: 16px 0 0 9px;
    }
    #tv-home {
        border-bottom: 2px solid #000000;
        height: 173px;
    }
    #tv-home > div {
        height: 173px;
    }
    .ui-swipeslide-bullets li {
        width: 14px;
        height: 16px;
        margin: 0 6px 0 0 !important;
        background-size: 14px 16px;
    }
    .ui-swipeslide-bullets li.active {
        background-size: 14px 16px;
    }
    #home h2 {
        background-size: 19px 31px;
        text-indent: 30px;
        font-size: 31px;
        background-position: 15px 13px;
        border-bottom: 5px solid #e64177;
        padding-left: 13px;
        padding-top: 7px;
    }
    #jqt ul#moteis-premium li {
        height: 140px;
        background-size: 100% 140px;
    }
    .premium-left {
        height: 87px;
        background-size: 132px 87px;
        margin-top: 28px;
    }
    .premium-left img {
        width: 100px;
        margin: 11px 0 0 17px;
    }
    .premium-left .distancia {
        font-size: 12px;
        margin: 4px 0 0 18px;
    }
    .premium-right {
        height: 94px;
    }
    .premium-right .nome {
        margin: 31px 0 0 7px;
        font-size: 22px;
    }
    .premium-right .endereco {
        margin: 0px 0 0 7px;
        font-size: 16px;
    }
    #go-site {
        margin-top:22px;
    }
    #go-site .versao-desktop {
        width:220px;
        height:62px;
        background-size: 220px 62px;
    }
    #go-site .topo {
        width:204px;
        height:62px;
        background-size: 204px 62px;
    }
    #go-site .versao-desktop a, #go-site .topo a{
            height:62px;
    }
}
