#status {
    display:none;
}
#wrapper {
    position:relative;
}
#slidemenu {
    position:absolute;
}
figure {
    margin:0;
    padding:0;
    position:relative;
}
 #jqt .toolbar {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-box-shadow: rgba(0, 0, 0, 0.4) 0 1px 6px;
    box-shadow: rgba(0, 0, 0, 0.4) 0 1px 6px;
    border-bottom: 1px solid #2a3441;
    z-index: 10;
    position: relative;
    padding: 10px;
    background: url(../img/mobile-menu-bg.jpg) repeat-x;
}
#menu-bt, .mobile-logo, .search {
    position: relative;
    display: inline-block;
}
#menu-bt {
    background: url(../img/mobile-menu.png) no-repeat;
}
.search {
    background: url(../img/mobile-search.png) no-repeat;
    border: 0;
    float: right;
}
.bt-search {
    background: url(../img/bt-search.png) no-repeat;
    border: 0;
    float: right;
}
#tv-home {
    margin: 0;
    padding: 0;
}
ul.ui-swipeslide-bullets {
    margin-top: -22px !important;
    position: absolute;
    background: none !important;
    border: none !important;
    width: 100%;
}
figcaption {
    position: absolute;
    bottom: 20px;
    left: 0;
    top: 100%;
    padding: 10px 20px;
    color: #fff;
    background-color: rgba(0,0,0,0.7);
    z-index: 999999;
    float: left;
    margin-top: -56px;
}
.ui-swipeslide-bullets li {
    background: url(../img/bullets.png) no-repeat;
    border: none !important;
    border-radius: 0 !important;
    padding: 0 !important;
}
.ui-swipeslide-bullets li.active {
    background: url(../img/bullets-active.png) no-repeat;
}
#home h2 {
    background: url(../img/icon-proximos.png) no-repeat;
    color: #dd0048;
    font-weight: 900;
    font-family: "Arial Black", Arial, sans-serif;
    display: inline-block;
}
#jqt ul#moteis-premium li {
    border-bottom: 1px solid #e2e2e2;
    background: url(../img/premium-bg.png) repeat-x;      
}
.premium-left {
    float: left;
    width: 28%;
    background: url(../img/premium-left.png) no-repeat;
}
    .premium-left img {
    float: left;
    height: auto;
    clear: both;
}
.premium-left .distancia {
    float: left;
    clear: both;
    color: #282828;
    font-family: Arial, Helvetica, sans-serif;  
}
.premium-right {
    float: left;
    width: 72%;
    color: #282828;
    background: url(../img/premium-arrow.png) no-repeat 95% 37px;
}
.premium-right .nome {
    float: left;
    clear: both;
    font-weight: bold;
}
.premium-right .endereco {
    float: left;
    clear: both;
}
#go-site .versao-desktop {
    background: url(../img/versao-desktop.png) no-repeat;
}
#go-site .topo {
    background: url(../img/topo-da-pagina.png) no-repeat;
}
#go-site .versao-desktop a, #go-site .topo a{
    width:100%;
}
/* Versão portrait pra iphone - largura 320px*/
@media screen and (max-width: 320px) {
    #wrapper {
        width:320px;
    }
    #jqt .toolbar {
        height: 60px;
        background-size: auto 60px;
    }
    #menu-bt {
        width: 20px;
        height: 20px;
        background-size: 20px 20px;
        top: -29px;
    }
    .mobile-logo {
        width: 65px;
        height: 61px;
        top: -4px;
    }
    .search {
        width: 169px;
        height: 28px;
        background-size: 169px 28px;
        margin-top: -58px;
        margin-right: 39px;
        text-indent: 5px;
    }
    .bt-search {
        width: 35px;
        height: 28px;
        background-size: 35px 28px;
        margin: 8px 0 0 4px;
    }
    #tv-home {
        border-bottom: 2px solid #000000;
        height: 116px;
    }
    #tv-home > div {
        height: 116px;
    }
    .ui-swipeslide-bullets li {
        width: 10px;
        height: 10px;
        margin: 0 3px 0 0 !important;
        background-size: 11px 11px;
    }
    .ui-swipeslide-bullets li.active {
        background-size: 11px 11px;
    }
    #home h2 {
        background-size: 13px 21px;
        text-indent: 21px;
        font-size: 21px;
        background-position: 13px 11px;
        border-bottom: 3px solid #e64177;
        padding-left: 13px;
        padding-top: 6px;
    }
    #jqt ul#moteis-premium li {
        height: 93px;
        background-size: 100% 93px;
    }
    .premium-left {
        height: 59px;
        background-size: 88px 59px;
        margin-top: 18px;
    }
    .premium-left img {
        width: 66px;
        margin: 6px 0 0 11px;
    }
    .premium-left .distancia {
        font-size: 8px;
        margin: 6px 0 0 14px;
    }
    .premium-right {
        height: 94px;
    }
    .premium-right .nome {
        margin: 23px 0 0 7px;
        font-size: 15px;
    }
    .premium-right .endereco {
        margin: 0px 0 0 7px;
        font-size: 11px;
    }
    #go-site {
        margin-top:22px;
    }
    #go-site .versao-desktop {
        width:147px;
        height:43px;
        background-size: 147px 43px;
    }
    #go-site .topo {
        width:136px;
        height:43px;
        background-size: 136px 43px;
    }
    #go-site .versao-desktop a, #go-site .topo a{
        height:43px;
    }
}
/* Versão Landscape pra iphone - largura 480px*/
@media only screen and (min-width : 321px) and (max-width : 480px) {
